{
	"env": {
		"browser": true,
		"commonjs": true,
		"es6": true,
		"node": true
	},
	"globals": {
		"ePub": true,
		"JSZip": true
	},
	"extends": ["eslint:recommended", "prettier"],
	"parserOptions": {
		"sourceType": "module"
	},
	"rules": {
		"no-unused-vars": ["warn"],
		"no-console": ["warn"],
		"valid-jsdoc": ["warn"]
	}
}
